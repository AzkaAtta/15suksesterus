---
import DefaultLayout from '@layouts/DefaultLayout.astro'
import SiteMeta from '@components/SiteMeta.astro'
import LayananSchema from '@components/LayananSchema.astro'
---

<DefaultLayout title="Layanan Aura Digital">
  <Fragment slot="head">
    <SiteMeta
      title="Layanan Jasa Sosial Media Murah & Terpercaya | Aura Digital"
      description="Temukan berbagai layanan sosial media murah, aman, dan terpercaya: komentar Instagram, followers Instagram, views TikTok, trending topik, hingga jasa report."
      url="https://auradigital.id/layanan"
      image="/layanan.png"
      siteName="Aura Digital"
      ogType="website"
      twitterCard="summary_large_image"
      keywords={[
        "jasa sosial media murah",
        "jasa komentar instagram",
        "beli followers instagram",
        "jasa views tiktok",
        "jasa trending topik",
        "jasa report akun"
      ]}
    />
    <LayananSchema
      name="Layanan Sosial Media"
      description="Daftar lengkap layanan sosial media dari Aura Digital: komentar Instagram, followers, views TikTok, trending topik, dan lain-lain."
      image={["https://auradigital.id/layanan.webp", "https://auradigital.id/layanan.png"]}
      url="https://auradigital.id/layanan"
    />
  </Fragment>

  <section>
    <h1>Layanan Kami</h1>
    <ul>
      <li><a href="/komentar-instagram/">Komentar Instagram</a></li>
      <li><a href="/followers-instagram/">Followers Instagram</a></li>
      <li><a href="/views-tiktok/">Views TikTok</a></li>
      <li><a href="/trending-topik/">Trending Topik</a></li>
      <li><a href="/jasa-report/">Jasa Report</a></li>
    </ul>
  </section>
</DefaultLayout>
